{
  "name": "ngx-agora",
  "version": "0.0.1",
  "schematics": "./schematics/src/collection.json",
  "peerDependencies": {
    "@angular/common": "^7.2.0",
    "@angular/core": "^7.2.0",
    "agora-rtc-sdk": "^2.5.2"
  },
  "description": "Angular 7 wrapper for Agora RTC client (https://www.agora.io/en/)",
  "main": "ngx-agora.module.ts",
  "devDependencies": {
    "agora-rtc-sdk": "^2.5.2"
  },
  "scripts": {
    "test": "test",
    "build": "../../node_modules/.bin/tsc -p schematics/tsconfig.json",
    "copy:schemas": "ditto schematics/src/*/schema.json ../../dist/ngx-agora/",
    "copy:collection": "cp schematics/src/collection.json ../../dist/ngx-agora/schematics/collection.json",
    "postbuild": "mkdir ../../dist/ngx-agora/schematics npm run copy:schemas && npm run copy:collection"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drew-thompson/ngx-agora.git"
  },
  "keywords": [
    "angular",
    "library",
    "agora",
    "agora-rtc",
    "agoraio",
    "video",
    "conferencing",
    "typescript"
  ],
  "author": "Drew Thompson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drew-thompson/ngx-agora/issues"
  },
  "homepage": "https://github.com/drew-thompson/ngx-agora#readme"
}
